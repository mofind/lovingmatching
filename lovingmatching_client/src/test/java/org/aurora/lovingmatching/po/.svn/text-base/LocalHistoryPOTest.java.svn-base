public class LocalHistoryPOTest extends TestCase {
	LocalHistoryPO localHistoryPO = new LocalHistoryPO("testID", 100, 200, 5);

	public void testToStoreString() {
		String storeString = localHistoryPO.toStoreString();
		System.out.println(storeString);
		assertNotNull(storeString);
	}

	public void testToConsoleString() {
		String consoleString = localHistoryPO.toConsoleString();
		System.out.println(consoleString);
		assertNotNull(consoleString);
	}

}
