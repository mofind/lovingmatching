package org.aurora.lovingmatching.view.compLibView;

import org.aurora.lovingmatching.view.roomAndHallView.RoomPanel;

/**
 * @Description 为提高效率，存储房间信息
 * @author m2o2o2d
 * 2014年5月17日上午12:00:00
 */
public class RoomInfo {

	private String num;
	private String name;
	private String people;
	
	public RoomInfo(String num,String name,String people) {
		this.num = num;
		this.name = name;
		this.people = people;
	}
	
	/*Getter&Setter*/
	public String getNum() {
		return num;
	}

	public void setNum(String num) {
		this.num = num;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPeople() {
		return people;
	}

	public void setPeople(String people) {
		this.people = people;
	}
	
}
